# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/log/benchmark/*.log
/log/*.output
/log/*.png
/tmp
/.idea
/coverage

# Ignore local development collection storage
dev/**

# Ignore local public files
public/assets/**
public/collections/**
public/uploads/**

# Ignore activemq, which can be installed manually
apache-activemq-5.8.0/**

# Ignore submodule jetty
jetty/**

# Ignore test directory
test/**

*.swp

